var stage:Stage;
function create() {
    stage = new Stage("stage");
    add(stage);
}

function beatHit(curBeat)
{
    if (curBeat % 8 == 7)
    {
        if(PlayState.current.bf != null)
        {
            PlayState.current.bf.playAnim('hey', true);
            PlayState.current.bf.specialAnim = true;
        }
    }
}