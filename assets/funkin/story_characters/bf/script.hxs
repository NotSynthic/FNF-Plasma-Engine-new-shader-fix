function create() {
    frames = FNFAssets.getStoryCharacterSparrow("bf");
    scale.set(0.85, 0.85);
    updateHitbox();
    animation.addByPrefix("idle", "BF idle dance white", 24, false);
    animation.addByPrefix("confirm", "BF HEY!!", 24, false);

    setOffset("idle");
    setOffset("confirm");

    dance();
}